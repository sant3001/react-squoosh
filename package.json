{
  "name": "react-squoosh",
  "version": "0.0.3",
  "description": "React Squoosh is an image compression utility for React that uses WebAssembly.",
  "main": "src/index.js",
  "unpkg": "src/index.js",
  "types": "src/index.d.ts",
  "author": "Santiago Bendavid",
  "license": "Apache-2.0",
  "files": [
    "src"
  ],
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/react": "^16.9.34",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel": "^6.23.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "prettier": "^2.0.5",
    "react": "^16.4.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {},
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "lint": "eslint --ext .ts,js,jsx,tsx src/",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"**/*.+(ts|tsx|js|jsx|json|css|md)\"",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "webpack --config webpack.config.js",
    "prebuild": "rimraf ./dist && npm run lint:fix",
    "build": "npm run build:types && npm run build:js",
    "postbuild": "cp package.json dist/ && cp README.md dist/ && cp LICENSE dist/ && cp -r public dist/",
    "deploy": "npm run build && cd dist && npm publish"
  },
  "keywords": [
    "react",
    "squoosh",
    "image compression",
    "WebAssembly"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sant3001/react-squoosh.git"
  }
}
